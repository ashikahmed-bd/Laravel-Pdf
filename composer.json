{
    "name": "ashik/pdf",
    "description": "A custom Laravel package for generating PDFs using mPDF with Facades and a Wrapper",
    "type": "library",
    "license": "MIT",

    "authors": [
        {
            "name": "Ashik Ahmed",
            "email": "md.ashikahmed@outlook.com"
        }
    ],
    "minimum-stability": "dev",

    "require": {
        "php": "^8.0",
        "mpdf/mpdf": "v8.2.1",
        "ext-openssl": "*"
    },
    "autoload": {
        "psr-4": {
            "Ashik\\Pdf\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Ashik\\Pdf\\PdfServiceProvider"
            ],
            "aliases": {
                "PDF": "Ashik\\Pdf\\Facades\\Pdf"
            }
        }
    }
}
